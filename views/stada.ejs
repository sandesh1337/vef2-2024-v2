<%- include('header') -%>

<h1>Staðan í deildinni</h1>

<table id="stada-table">
    <thead>
    <tr>
        <th>Team</th>
        <th>Score</th>
    </tr>
    </thead>
    <tbody>
    <% for (const stada of locals.stada) { %>
        <tr>
            <td><%= stada.name %></td>
            <td><%= stada.points %></td>
        </tr>
    <% } %>
    </tbody>
</table>



<%- include('footer') -%>
